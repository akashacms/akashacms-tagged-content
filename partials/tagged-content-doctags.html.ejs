<span class="taglist">
    <% for (var i = 0; i < tagz.length; i++) {
        var tagData = tagz[i];
        %> <a style="padding: 5px;" class="taglink-header p-category" href="<%= tagData.tagUrl %>"><%= tagData.tagName %></a> <%
        if (i < (tagz.length-1)) { %> &raquo;&raquo;&raquo;&raquo; <% }
    } %>
</span>