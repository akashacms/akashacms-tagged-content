<div <% if (typeof id !== 'undefined') { %>id="<%= id %>" <% } %> class="<%= additionalClasses %>">
<ul>
    <% for (var tagEntry of entries) {
        %><li><a rel="alternate" type="application/rss+xml" href="<%= tagEntry.rssHREF %>"><%=
            tagEntry.tagName
        %></a><% if (typeof tagEntry.teaser !== 'undefined') {
            %>: <%= tagEntry.teaser %><%
        }
        %></li>
    <% } %>
</ul>
</div>