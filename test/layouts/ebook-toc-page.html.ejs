<!doctype html>
<!-- paulirish.com/2008/conditional-stylesheets-vs-css-hacks-answer-neither/ -->
<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]>    <html class="no-js lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]>    <html class="no-js lt-ie9" lang="en"> <![endif]-->
<!-- Consider adding a manifest.appcache: h5bp.com/d/Offline -->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
<head>
<meta charset="utf-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta name="viewport" content="width=device-width" />

<% if (typeof pagetitle !== "undefined") { %>
    <title><%= pagetitle %></title>
<% } else if (typeof title !== "undefined") { %>
    <title><%= title %></title>
<% } %>
<%- partialSync("ak_headermeta.html.njk", locals) %>
<!-- <ak-header-metatags></ak-header-metatags> -->
<link rel="sitemap" type="application/xml" href="/sitemap.xml"/>
<%- config.plugin("@akashacms/plugins-base").doLinkRelTags() %>
<%- config.plugin("@akashacms/plugins-base").doCanonicalURL(rendered_url) %>
<!--
<xml-sitemap></xml-sitemap>
<site-verification></site-verification>
<ak-header-linkreltags></ak-header-linkreltags>
-->
<%- config.plugin("akashacms-builtin").doStylesheets(locals) %>
<!-- ak-stylesheets></ak-stylesheets -->
<%- config.plugin("akashacms-builtin").doHeaderJavaScript(locals) %>
<!-- ak-headerJavaScript></ak-headerJavaScript -->
<open-graph-promote-images root="div.panel-default"></open-graph-promote-images>

</head>
<body class="ebook-toc-page">

    <partial file-name="headernav.html.ejs"></partial>

<div class="container" role="main">
<div class="row">
<ebook-page-header class="col-sm-12"></ebook-page-header>
</div>

<div class="row">
  <article class="col-sm-8">
    <div class="panel panel-default">
        <ebook-nameplate-block></ebook-nameplate-block>
        <div class="ebook-toc-page-toc">
        <%- content %>
        </div>
        <!-- book-toc-list-group></book-toc-list-group -->
    </div>
  </article>
  <div class="col-sm-4">
      <% if (typeof coverImage !== 'undefined') { %><img src="<%= coverImage %>" width="100%"/><% } %>
  </div>
</div>

<footer class="row" style="text-align: center; height: 100px;">
<div class="col-sm-12">
</div>
</footer>
</div>

<%- config.plugin("akashacms-builtin").doFooterJavaScript(locals) %>
<!-- <ak-footerJavaScript></ak-footerJavaScript> -->



</body>
</html>
